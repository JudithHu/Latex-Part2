\end{mfpic}}

\begin{veta}[Fubini]\DUL
Nech» $f$ je funkce spojitá v~uzavøené oblasti  
\begin{equation*}
  \Omega=\{(x,y)\in\R^2:{a\leq x\leq b}\text{ a }
  {\phi(x)\leq y\leq
  \psi (x)}\}.
\end{equation*}
Potom 
\begin{equation*}
  \iint_{\Omega}f(x,y)\dx\dy={\int_{a}^{b}}
  \Bigl[ \int_{\phi(x)}^{\psi(x)}   
  f(x,y){\dy}\Bigr]{\dx}
\end{equation*}
\end{veta}
