\section{Supremum a infimum}



 \begin{definice}[dolní závora]
   Buï $A$ neprázdná zdola ohranièená mno¾ina reálných
   èísel. Èíslo $m$ se nazývá \textit{dolní závora} mno¾iny $A$,
   jestli¾e $m\leq a$ pro v¹echna $a\in A$
 \end{definice}

 \begin{priklad}
   \begin{itemize}
   \item Dolní závorou intervalu $(0,1)$ jsou napøíklad èísla
     $-1,-\pi,0$. 
   \item Dolní závorou nejsou èísla $\frac 12$, $6$ ani
     $e$.
 \end{itemize}
 \end{priklad}

 \begin{definice}[infimum]
   Buï $A$ neprázdná zdola ohranièená mno¾ina reálných
   èísel. Èíslo $\inf(A)$ se nazývá \textit{infimum} mno¾iny $A$,
   jestli¾e je nejvìt¹í dolní závorou mno¾iny $A$.
 \end{definice}


 \begin{priklad}
   Intervaly $(0,1)$, $[0,1]$, $(0,1]$ mají v¹echny infimum rovno
   èíslu $0$.
 \end{priklad}



 \begin{definice}[horní závora]
   Buï $A$ neprázdná shora ohranièená mno¾ina reálných
   èísel. Èíslo $M$ se nazývá \textit{horní závora} mno¾iny $A$,
   jestli¾e $M\geq a$ pro v¹echna $a\in A$
 \end{definice}

 \begin{definice}[supremum]
   Buï $A$ neprázdná shora ohranièená mno¾ina reálných
   èísel. Èíslo $\sup(A)$ se nazývá \textit{supremum} mno¾iny $A$,
   jestli¾e je nejmen¹í horní závorou mno¾iny $A$.
 \end{definice}

 \begin{priklad}
   Intervaly $(0,1)$, $[0,1]$, $(0,1]$ mají v¹echny supremum rovno
   èíslu $1$.
 \end{priklad}



\section{Dvojný integrál na obdélníku}

Definujme funkci na obdélníku $R=[a,b]\times [c,d]$ ohranièenou
funkci $f(x,y)$. Obdélník rozdìlme na podobdélníky $p_1$, $p_2$,
\dots, $p_n$ o~obsazích $\Delta p_1$, $\Delta p_2$, \dots, $\Delta
p_n$. Toto dìlení oznaème $D$.

V~obdélníèku $p_i$ najdeme supremum $M_i$ a infimum $m_i$ funkce
$f(x,y)$. Sestrojme horní a dolní integrální souèet pøíslu¹ný
dìlení $D$ podle vzorcù\DUL
\begin{align*}
  S(D)=\sum_{i=1}^k M_i\Delta p_i&\text{ \dots horní souèet}\\
  s(D)=\sum_{i=1}^k m_i\Delta p_i&\text{ \dots dolní souèet}
\end{align*}
\begin{itemize}
\item Supremum mno¾iny v¹ech dolních souètù nazýváme \textit{dolní
    dvojný integrál} a znaèíme
  $\underline{\iint_{R}}f(x,y)\dx\dy$.
\item Infimum mno¾iny v¹ech horních souètù nazýváme \textit{horní
    dvojný integrál} a znaèíme $\overbar{\iint_{R}}f(x,y)\dx\dy$.
\end{itemize}

\begin{definice}[dvojný integrál]
  Jestli¾e jsou si horní a dolní integrál rovny, pak jejich
  spoleènou hodnotu znaèíme 
  \begin{equation}
    \label{eq:dvojint}
    \iint_R f(x,y)\dx\dy
  \end{equation}
  a nazýváme \textit{dvojný integrál funkce $f$ v $R$.} O~funkci
  $f$ øíkáme, ¾e je na mno¾inì $R$ \textit{integrovatelná}.
\end{definice}

Výpoèet dvojného integrálu provádíme s~vyu¾itím následující vìty
o~pøevodu dvojného integrálu na dvojnásobný (dva \uv{obyèejné}
integrály). 

\begin{veta}[Fubini]\DUL
  Nech» $R=[a,b]\times[c,d]$ je uzavøený obdélník v $\R^2$ a $f$
  funkce definovaná a spojitá na $R$. Pak platí
  \begin{equation*}
    \iint_R f(x,y)\dx\dy
    =
    \int_a^b\Bigl[\int_c^d f(x,y)\dy\Bigr]\dx
    =
    \int_c^d\Bigl[\int_a^b f(x,y)\dx\Bigr]\dy.
  \end{equation*}
\end{veta}

\begin{priklad}
Vypoètìte $\iint_\Omega (x+y)\dx\dy$ pøes obdélník vyznaèený na
obrázku. 

\def\Od #1 {}  
\def\zelena[#1]{}
\def\cervena[#1]{}
\hbox{\obrazek{\begin{mfpic}{-1}{4}{-0.5}{2.5} \axes \tlabelsep{3pt}
       \tlabel[tc](3,0){$3$} \tlabel[cr](0,1){$1$}
       \tlabel[cr](0,2){$2$} \tlabel[cl](4,0){$x$}
       \tlabel[tl](0,2.5){$y$} \tlabel[cc](1.5,1.5){$\Omega$}
      \rhatch\btwnfcn{0,3,1}{1}{2} \pen{2pt}
      \lines{(0,1),(3,1),(3,2),(0,2),(0,1)}
      \dotted\lines{(3,1),(3,0)} 
      \headlen = 6pt
      {\arrow\reverse\arrow\lines{(0,0),(3,0)}}
      {\arrow\reverse\arrow\lines{(0,1),(0,2)}}
  \end{mfpic}}}

  \begin{align*}
    \iint_\Omega (x+y)\dx\dy&\Od 2 ={\zelena[2-]\int_1^2}\Bigl[
    {\cervena[2-]\int_0^3}
    (x+y){\cervena[2-]\dx}\Bigr]{\zelena[2-]\dy}
    \Od 3 =\int_1^2\Bigl[\frac{x^2}2+xy\Bigr]_0^3\dy\\
    &\Od 4 =\int_1^2\Bigl[\frac 92+3y-\bigl(\frac
    02+0y\bigr)\Bigr]\dy \Od 5 =\int_1^2\Bigl(\frac 92+3y\Bigr)\dy\\
    &\Od 6 =\Bigl[\frac 92 y+3\frac{y^2}2\Bigr]_1^2
    \Od 7 =\frac 92\cdot 2+3\cdot\frac 42-\Bigl(\frac 92+3\cdot\frac 12\Bigr)\\
    &\Od 8 =9+6-6=9
  \end{align*}

\end{priklad}

\section{Dvojný integrál v~obecné oblasti}

\begin{definice}[dvojný integrál v~obecné oblasti]
  Buï $\Omega$ uzavøená ohranièená oblast. Buï $R$ dostateènì
  velký obdélník, takový, ¾e $\Omega\subseteq R$. Definujme na $R$
  funkci $g$ pøedpisem
  \begin{equation*}
    g(x,y)=
    \begin{cases}
      f(x,y)&(x,y)\in \Omega\\
      0&\text{jinak}
    \end{cases}
  \end{equation*}
  Potom definujeme integrál funkce $f$ na mno¾inì $\Omega$
  pøedpisem 
  \begin{equation*}
    \iint_\Omega f(x,y)\dx\dy=\iint_R g(x,y)\dx\dy.
  \end{equation*}
\end{definice}

V~dal¹ím budeme pro jednoduchost pøedpokládat, ¾e oblasti pøes
které integrujeme 
mají hranici tvoøenu po èástech hladkou uzavøenou køivkou.


