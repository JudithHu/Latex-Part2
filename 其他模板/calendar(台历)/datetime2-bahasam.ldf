\DTMnewstyle
 {bahasam}% label
 {% date style
   \renewcommand*\DTMdisplaydate[4]{%
     \DTMifbool{bahasam}{showdayofmonth}
     {\DTMbahasamordinal{##3}\DTMbahasamdaymonthsep}%
     {}%
     \DTMifbool{bahasam}{abbr}%
       {\DTMbahasamshortmonthname{##2}}%
       {\DTMbahasammonthname{##2}}%
     \DTMifbool{bahasam}{showyear}%
     {%
       \DTMbahasammonthyearsep
       \number##1 % space intended
     }%
     {}%
     \ifDTMshowdow
       \ifnum##4>-1
         \space(%
         \DTMifbool{bahasam}{abbr}%
         {\DTMbahasamshortweekdayname{##4}}%
         {\DTMbahasamweekdayname{##4}})%
       \fi
     \fi
   }%
   \renewcommand*\DTMDisplaydate{\DTMdisplaydate}%
 }%
 {% time style (use default)
   \DTMsettimestyle{default}%
 }%
 {% zone style
   \DTMresetzones
   \DTMbahasamzonemaps
   \renewcommand*{\DTMdisplayzone}[2]{%
     \DTMifbool{bahasam}{mapzone}%
     {\DTMusezonemapordefault{##1}{##2}}%
     {%
       \ifnum##1<0\else+\fi\DTMtwodigits{##1}%
       \ifDTMshowzoneminutes\DTMbahasamtimesep\DTMtwodigits{##2}\fi
     }%
   }%
 }%
 {% full style
   \renewcommand*{\DTMdisplay}[9]{%
    \ifDTMshowdate
     \DTMdisplaydate{##1}{##2}{##3}{##4}%
     \DTMbahasamdatetimesep
    \fi
    \DTMdisplaytime{##5}{##6}{##7}%
    \ifDTMshowzone
     \DTMbahasamtimezonesep
     \DTMdisplayzone{##8}{##9}%
    \fi
   }%
   \renewcommand*{\DTMDisplay}{\DTMdisplay}%
 }%
