\newcommand*\DTMbahasamam{am}%
\newcommand*\DTMbahasampm{pm}%
\newcommand*\DTMbahasammidnight{t.m.}%
\newcommand*\DTMbahasamnoon{t.h.}%
\newcommand*{\DTMbahasamampmfmt}[1]{#1}
\newcommand*{\DTMbahasamtimesep}{\DTMsep{hourmin}}
\DTMnewtimestyle
 {bahasamampm}% label
 {%
    \renewcommand*\DTMdisplaytime[3]{%
      \ifnum##2=0
        \ifnum##1=12
          \DTMtexorpdfstring
            {\DTMbahasamampmfmt{\DTMbahasamnoon}}%
            {\DTMbahasamnoon}%
        \else
          \ifnum##1=0
            \DTMtexorpdfstring
            {\DTMbahasamampmfmt{\DTMbahasammidnight}}%
            {\DTMbahasammidnight}%
          \else
            \ifnum##1=24
              \DTMtexorpdfstring
              {\DTMbahasamampmfmt{\DTMbahasammidnight}}%
              {\DTMbahasammidnight}%
            \else
              \ifnum##1<12
                \number##1
                \DTMtexorpdfstring
                {\DTMbahasamampmfmt{\DTMbahasamam}}%
                {\DTMbahasamam}%
              \else
                \number\numexpr##1-12\relax
                \DTMtexorpdfstring
                {\DTMbahasamampmfmt{\DTMbahasampm}}%
                {\DTMbahasampm}%
              \fi
            \fi
          \fi
        \fi
      \else
        \ifnum##1<13
          \ifnum##1=0
            12%
          \else
            \number##1
          \fi
          \DTMbahasamtimesep\DTMtwodigits{##2}%
          \ifnum##1=12
            \DTMtexorpdfstring
            {\DTMbahasamampmfmt{\DTMbahasampm}}%
            {\DTMbahasampm}%
          \else
            \DTMtexorpdfstring
            {\DTMbahasamampmfmt{\DTMbahasamam}}%
            {\DTMbahasamam}%
          \fi
        \else
          \number\numexpr##1-12\relax
          \DTMbahasamtimesep\DTMtwodigits{##2}%
          \ifnum##1=24
            \DTMtexorpdfstring
            {\DTMbahasamampmfmt{\DTMbahasamam}}%
            {\DTMbahasamam}%
          \else
            \DTMtexorpdfstring
            {\DTMbahasamampmfmt{\DTMbahasampm}}%
            {\DTMbahasampm}%
          \fi
        \fi
      \fi
    }%
 }%
