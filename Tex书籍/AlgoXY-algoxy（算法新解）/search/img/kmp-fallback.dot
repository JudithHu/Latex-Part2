digraph G{
	margin="0"
	node[shape=record]
        hdp[shape=point];
        w[label="<h> ...| <1> T[i]| <2> T[i+1]| T[i+2] | ... ... ... ... | <q> T[i+q-1]|..."];
        T[shape=plaintext];
        p[label="<1> P[1]| <2> P[2]|...|<k> P[j]| <k1> P[j+1]|...| <q> P[q]|..."];
        P[shape=plaintext];
        hdpk[shape=point];
        pk[label="<1> P[1]| <2> P[2]|...| <k> P[k]|..."];
        Pk[label="P", shape=plaintext];
        w:h->hdp[style=invis];
        w:1->p:1;
        w:2->p:2;
        w:q->p:q;
        p:1->hdpk[style=invis];
        p:k->pk:1;
        p:k1->pk:2;
        p:q->pk:k;
        {rank=same w T}
        {rank=same hdp p P}
        {rank=same hdpk pk Pk}
        hdp->p[label="s"];
        hdpk->pk;
}
