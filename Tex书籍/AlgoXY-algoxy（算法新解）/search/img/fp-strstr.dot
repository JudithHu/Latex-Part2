digraph G{
	margin="0"
	node[shape=record]
        hdp[shape=point];
        w1[label="<h> T[1]| T[2] | ... ... | T[i-1]|<ti> T[i]"];
        w2[label="<1> T[i+1] | T[i+2] | ... ... | T[n-1] | T[n]"];
        T[shape=plaintext];
        
        p1[label="<1> P[1]| P[2]| ... |<pj> P[j]"];
        p2[label="<1> P[j+1] | P[j+2] | ... | P[m]"];
        P[shape=plaintext];
        
        w1:h->hdp[style=invis];
        w1:ti->p1:pj;
        w2:1->p2:1[label="?"];
        {rank=same w1 w2 T}
        {rank=same hdp p1 p2 P}
        hdp->p1[label="s"];
}
